<% for(var i=0;i<devices.length;i++){%>
    <tr>
        <th><%= i+1 %></th>
        <td><%= devices[i].ident_personne.name %></td> 
        <td><%= devices[i].imei %></td>
        <td><%= devices[i].device_nom %></td>
        <td><%= devices[i].ident_group.group_nom  %></td>
        <td><%= devices[i].ident_canal.canal_nom %></td>
        <td>
          <a href="/device/admin/update-device?id=<%= devices[i]._id %>" class="btn border-shadow update">
            <span class="text-gardient"><i class="fas fa-pencil-alt"></i></span>
          </a>
          <a  class="btn border-shadow delete" data-id=<%= devices[i]._id %> >
            <span class="text-gardient"><i class="fas fa-times"></i></span>
          </a>
          <a href="/stream/admin/lister_stream">  
            <button>Log</button>  
          </a>
        </td>  
    </tr>
    <% } %>
    